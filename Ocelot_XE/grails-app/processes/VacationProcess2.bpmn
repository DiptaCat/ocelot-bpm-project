      <bpmndi:BPMNShape id="BPMNShape_sendConfirmationE-mail" bpmnElement="sendConfirmationE-mail">
        <omgdc:Bounds height="60.0" width="100.0" x="470.0" y="0.0"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="BPMNShape_end0" bpmnElement="end0">
